.header {
    display: flex;
    height: 60rpx;
    line-height: 60rpx;
    padding: 10rpx;
}

.searchInput {
    flex: 1;
    background-color: rgba(237, 237, 237, 0.3);
    border-radius: 30rpx;
}

.cancel {
    width: 100rpx;
    text-align: center;
}

.searchIcon {
    position: absolute;
    left: 15rpx;
}

.searchInput input {
    margin-left: 50rpx;
    height: 60rpx;
}

.placeholder {
    font-size: 28rpx;
}